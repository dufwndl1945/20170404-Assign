public class Student1 {

    String hakga;
    int hakbun;

    public static void main(String[] args) {
        Student1 student = new Student1();
        student.hakga = "컴퓨터정보공학과";
        student.hakbun = 20132773;

        System.out.println("학번: " + student.hakga + ", 학과: " + student.hakbun);
    }
}
